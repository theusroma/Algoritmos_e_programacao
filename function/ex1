#include <stdio.h>
#include <locale.h>

int main (){
	
	setlocale(LC_ALL, "Portuguese");
	
	int i;


	printf("* * * * * * * * * *");
	
	printf("\n");
	
	printf("NÃºmeros entre 1 e 5: ");
	
	printf("\n");

	printf("* * * * * * * * * *");
	
	printf("\n");
	
	for(i = 1; i <= 5; i++){
		printf("%d ", i);
	}
	
	printf("\n");
	
	printf("* * * * * * * * * *");
	
	printf("\n");
	
	return 0;
}
