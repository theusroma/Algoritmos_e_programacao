// Online C compiler to run C program online
#include <stdio.h>
#include <locale.h>

int main() {
   
   int num;
   
    printf("Informe o numero: ");
    scanf("%d", &num);
    
    if (num % 6 == 0){
        
        printf("O numero  e divisivel por 2 e 3");
    }
        
          else {
        printf("O numero não é divisível por 2 e 3");
    }
    
    
    


    return 0;
}
