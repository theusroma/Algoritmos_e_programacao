//Escreva um programa que alimente um vetor com 5 números inteiros. Apresente na tela apenas os valores ímpares que aparecem no vetor
//mostre só os ímpares
#include <stdio.h>

int main() {
    
    int i;
    int vetor[5];
    
    for(i = 0; i < 5; i++){
        printf("Digite um número: ");
        scanf("%d", &vetor[i]);
    }
    
    for(i = 0; i < 5; i++){
        if(vetor[i] % 2 != 0){
        printf("Elementos: %d ", vetor[i]);
    }
}
    
   return 0;
}
