//Escreva um programa que alimente um vetor com 5 números inteiros. Apresente na tela apenas os valores ímpares que aparecem no vetor
#include <stdio.h>

int main() {
    
    int contador, vetor[5];
    
    /*Alimenta o vetor*/
    for(contador = 0; contador < 5; contador = contador + 1){
        printf("Escreva número: ");
        scanf("%d", &vetor[contador]);
    }
    
    /*Mostra os elementos do vetor*/
    for(contador = 0; contador < 5; contador = contador + 1){
        if(vetor[contador] % 2 != 0){
        printf("\nElementos: %d", vetor[contador]);
    }
}



printf("\n\n\n\nDeu certo!!!");
    return 0;
}
