//Escreva um programa que alimente um vetor com 5 números inteiros e apresente na tela o vetor de forma inversa
#include <stdio.h>

int main() {
    
    int contador, vetor[5];
    
    /*Alimenta o vetor*/
    for(contador = 0; contador < 5; contador = contador + 1){
        printf("Escreva número: ");
        scanf("%d", &vetor[contador]);
    }
    
    /*Mostra os elementos do vetor*/
    for(contador = 4; contador >= 0; contador = contador - 1){
        printf("Elementos: %d\n", vetor[contador]);
    }

    return 0;
}
