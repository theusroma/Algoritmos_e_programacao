//Escreva um programa que alimente um vetor com 5 números inteiros e apresente na tela o vetor de forma inversa
#include <stdio.h>

int main() {
   
   int i;
   int vetor[5];
   
    for(i = 0; i < 5; i++){
        printf("Digite um número: ");
        scanf("%d", &vetor[i]);
    }
    
    for(i = 4; i >= 0; i--){
        printf("%d ", vetor[i]);
    }



    return 0;
}
