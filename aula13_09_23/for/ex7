// Online C compiler to run C program online
#include <stdio.h>

int main() {
    
    int num, total, resultado;
    
    printf("Digite um n√∫mero: ");
    scanf("%d", &num);
    
    for(total = 1; total <= 10; total = total + 1){
        
        resultado = num * total;
        printf("\n%d x %d = %d", num, total, resultado);
    }
    
    
    
    
    
