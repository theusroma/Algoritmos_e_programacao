// Online C compiler to run C program online
#include <stdio.h>

int main() {
    
    float altura, predio, sombra;
    
    printf("Quantos centímetros você mede?");
    scanf("%f", &altura);
    
    printf("Quantas vezes a sombra do prédio é maior que a sua?");
    scanf("%f", &sombra);
    
    predio = (altura * sombra);
    
    printf("A altura do prédio é de %.2f", predio);
    
    
    return 0;
}
